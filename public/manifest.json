{
  "manifest_version": 3,
  "name": "Megaman",
  "version": "1.0.0",
  "description": "Collect Slack messages and read / search offline.",
  "icons": {
    "16": "icon-16.png",
    "32": "icon-32.png",
    "64": "icon-64.png",
    "128": "icon-128.png"
  },
  "action": { "default_popup": "index.html" },
  "permissions": [
    "activeTab",
    "scripting",
    "storage"
  ]
}